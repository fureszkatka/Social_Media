.Users_content
    display: flex
    flex-direction: column
    font-size: 25px
    margin-left: 30px
    align-items: center
    height:1200px

.Users_middle
    display: flex
    flex-direction: column
    overflow-y: scroll
    height: 1200px
    background-color: rgba(102, 51, 153, 0.252)
    padding-left: 50px
    padding-right: 50px
    scrollbar-color: rebeccapurple green


.Users_list-elements
    background-color: rgba(3,1,6,0.91);
    display: flex
    flex-direction: column
    padding: 20px
    border-radius: 15px
    margin-bottom: 25px

.Users_list-element
    color: pink
    
.Users_list-profile-name
    display: flex
    flex-direction: row
    align-items: center

.Users_view-profile
    background-color: cyan
    width: 200px
    height: 50px
    text-decoration: none
    display: flex
    flex-direction: row
    justify-content: center
    align-items: center